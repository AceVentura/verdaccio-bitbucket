{
  "extends": "eslint-config-airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
  },
  "plugins": [
    "import"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "globals": {
    "__DEVELOPMENT__": true
  }
}
